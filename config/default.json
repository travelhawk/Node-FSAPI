{
    // Authentication key
    keys: ["12345"],
    /**
     * Allowed IP's or ranges
     * Can use * for wildcards, *.*.*.* for no restrictions
     */
    ips: [
        "*.*.*.*"
    ],
    /**
     * SSL Config
     * Set key and cert to absolute path if SSL used, false if not
     */
    ssl: {
        key: false,
        cert: false
    },
    // Port designation
    port: 5555,
    // Base directory
    base: "/baseDir",
    // Default create mode
    cmode: "0755"
}
